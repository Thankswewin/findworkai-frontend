{
  "name": "findworkai-frontend",
  "version": "1.0.0",
  "description": "FindWorkAI - AI-Powered Business Discovery & Lead Generation Platform by Pheelymon",
  "author": {
    "name": "Pheelymon",
    "url": "https://www.instagram.com/pheelymon.oftan"
  },
  "homepage": "https://findworkai.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/Thankswewin/findworkai-frontend"
  },
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:e2e": "playwright test",
    "test:ai-workflow": "playwright test ai-backend-integration.spec.ts --project=chromium",
    "test:ai-workflow:headless": "playwright test ai-backend-integration.spec.ts --project=chromium --headed",
    "test:ai-performance": "playwright test ai-performance.spec.ts --project=performance --timeout=300000",
    "test:ai-full": "playwright test ai-agent-workflow.spec.ts ai-backend-integration.spec.ts --project=ai-workflow",
    "test:ai-report": "powershell -ExecutionPolicy Bypass -File ./scripts/test-ai-workflow.ps1",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache"
  },
